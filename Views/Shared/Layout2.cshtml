<!doctype html>
<html class="no-js" lang="zxx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Laundry | Teamplate</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="manifest" href="site.webmanifest">
    <link rel="shortcut icon" type="image/x-icon" href="~/usercdn/assets/img/favicon.ico">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-LN+7fdVzj6u52u30Kp6M/trliBMCMKTyK833zpbD+pXdCLuTusPj697FH4R/5mcr" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/js/bootstrap.bundle.min.js" integrity="sha384-ndDqU0Gzau9qJ1lfW4pNLlhNTkCfHzAVBReH9diLvGRem5+R9g2FzA8ZGN954O5Q" crossorigin="anonymous"></script>

    <!-- CSS here -->
    <link rel="stylesheet" href="~/usercdn/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/slicknav.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/flaticon.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/progressbar_barfiller.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/gijgo.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/animate.min.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/animated-headline.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/magnific-popup.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/themify-icons.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/slick.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/nice-select.css">
    <link rel="stylesheet" href="~/usercdn/assets/css/style.css">
    <!-- Toastr CSS -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" rel="stylesheet" />

    <!-- jQuery (required by Toastr) -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <!-- Toastr JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
</head>
<body>
    <!-- ? Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="~/usercdn/assets/img/logo/loder.png" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->
    <header>
        <!-- Header Start -->
        <div class="header-area">
            <div class="main-header header-sticky">
                <!-- Logo -->
                <div class="header-left">
                    <div class="logo">
                        <a asp-controller="Home" asp-action="Index"><img src="~/usercdn/assets/img/logo/logo.png" alt=""></a>
                    </div>
                    <div class="menu-wrapper  d-flex align-items-center">
                        <!-- Main-menu -->
                        <div class="main-menu d-none d-lg-block">
                            <nav>
                                <ul id="navigation">
                                    <li class="@((ViewData["CurrentAction"] == "Index" && ViewData["CurrentController"] == "Home") ? "active" : "")"><a asp-controller="Home" asp-action="Index">Home</a></li>
                                    <li class="@((ViewData["CurrentAction"] == "About" && ViewData["CurrentController"] == "Home") ? "active" : "")"><a asp-controller="Home" asp-action="About">About</a></li>
                                    <li class="@((ViewData["CurrentAction"] == "Services" && ViewData["CurrentController"] == "Home") ? "active": "")"><a asp-controller="Home" asp-action="Services">Services</a></li>

                                    <li class="@((ViewData["CurrentAction"] == "Contact" && ViewData["CurrentController"] == "Home") ? "active" : "")"><a asp-controller="Home" asp-action="Contact">Contact</a></li>
                                    @if (accessor.HttpContext.Session.Keys.Contains("id"))
                                    {
                                        <li class="@((ViewData["CurrentAction"] == "ShowBookings" && ViewData["CurrentController"] == "Home") ? "active" : "")"><a asp-controller="Home" asp-action="ShowBookings">My Bookings</a></li>
                                    }
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
                <div class="header-right d-none d-lg-block">
                    @{
                        if (string.IsNullOrEmpty(accessor.HttpContext.Session.GetString("id")))
                        {
                            <a asp-controller="Home" asp-action="Register" class="header-btn1"> Signup</a>
                            <a asp-controller="Home" asp-action="Login" class="header-btn2">Login</a>
                        }
                        else
                        {
                            <a asp-controller="Home" asp-action="Profile" class="header-btn1">Welcome, @accessor.HttpContext.Session.GetString("name")</a>
                            <a asp-controller="Home" asp-action="Logout" class="header-btn2">Logout</a>
                        }
                    }

                </div>
                <!-- Mobile Menu -->
                <div class="col-12">
                    <div class="mobile_menu d-block d-lg-none"></div>
                </div>
            </div>
        </div>
        <!-- Header End -->
    </header>

    @RenderBody()


    <footer>
        <!-- Footer Start-->
        <div class="footer-area footer-padding">
            <div class="container">
                <div class="row d-flex justify-content-between">
                    <div class="col-xl-4 col-lg-4 col-md-4 col-sm-6">
                        <div class="single-footer-caption mb-50">
                            <div class="single-footer-caption mb-30">
                                <!-- logo -->
                                <div class="footer-logo mb-35">
                                    <a href="index.html"><img src="~/usercdn/assets/img/logo/logo2_footer.png" alt=""></a>
                                </div>
                                <div class="footer-tittle">
                                    <div class="footer-pera">
                                        <p>Laundry, in its simplest definition, is the process of washing clothes, linens, and other textiles. It can involve various methods, including washing, drying, and ironing, with the aim of removing dirt, stains, and odors, and keeping fabrics in good condition.</p>
                                    </div>
                                </div>
                                <!-- social -->
                                <div class="footer-social">
                                    <a href="#"><i class="fab fa-twitter"></i></a>
                                    <a href="https://bit.ly/sai4ull"><i class="fab fa-facebook-f"></i></a>
                                    <a href="#"><i class="fab fa-pinterest-p"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-4 col-md-4 col-sm-6">
                        <div class="single-footer-caption mb-50">
                            <div class="footer-tittle">
                                <h4>Services </h4>
                                <ul>
                                    @foreach (var services in ViewBag.Services)
                                    {
                                        <li><a asp-controller="Home" asp-action="Services">- @services.Name</a></li>
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-4 col-md-4 col-sm-6">
                        <div class="single-footer-caption mb-50">
                            <div class="footer-tittle">
                                <h4>Get in touch</h4>
                                <ul>
                                    <li class="number"><a href="#">(+92) 315 330-7757</a></li>
                                    <li><a href="#">laundry@gmail.com</a></li>
                                    <li><a href="#">789/A, Zameen k uper</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- footer-bottom area -->
        <div class="footer-bottom-area section-bg2" data-background="~/usercdn/assets/img/gallery/footer-bg.png">
            <div class="container">
                <div class="footer-border">
                    <div class="row d-flex align-items-center">
                        <div class="col-xl-12 ">
                            <div class="footer-copy-right text-center">
                                <p>
                                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                                    Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart" aria-hidden="true"></i> by <a href="https://github.com/MuhammadRafayShaikh" target="_blank">Unknown 🥺💔</a>
                                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer End-->
    </footer>
    <!-- Scroll Up -->
    <div id="back-top">
        <a title="Go to Top" href="#"> <i class="fas fa-level-up-alt"></i></a>
    </div>

    <!-- JS here -->

    <script src="~/usercdn/assets/js/vendor/modernizr-3.5.0.min.js"></script>
    <!-- Jquery, Popper, Bootstrap -->
    <script src="~/usercdn/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="~/usercdn/assets/js/popper.min.js"></script>
    <script src="~/usercdn/assets/js/bootstrap.min.js"></script>
    <!-- Jquery Mobile Menu -->
    <script src="~/usercdn/assets/js/jquery.slicknav.min.js"></script>

    <!-- Jquery Slick , Owl-Carousel Plugins -->
    <script src="~/usercdn/assets/js/owl.carousel.min.js"></script>
    <script src="./assets/js/slick.min.js"></script>
    <!-- One Page, Animated-HeadLin -->
    <script src="~/usercdn/assets/js/wow.min.js"></script>
    <script src="~/usercdn/assets/js/animated.headline.js"></script>
    <script src="~/usercdn/assets/js/jquery.magnific-popup.js"></script>

    <!-- Date Picker -->
    <script src="~/usercdn/assets/js/gijgo.min.js"></script>
    <!-- Nice-select, sticky -->
    <script src="~/usercdn/assets/js/jquery.nice-select.min.js"></script>
    <script src="~/usercdn/assets/js/jquery.sticky.js"></script>
    <!-- Progress -->
    <script src="~/usercdn/assets/js/jquery.barfiller.js"></script>

    <!-- counter , waypoint,Hover Direction -->
    <script src="~/usercdn/assets/js/jquery.counterup.min.js"></script>
    <script src="~/usercdn/assets/js/waypoints.min.js"></script>
    <script src="~/usercdn/assets/js/jquery.countdown.min.js"></script>
    <script src="~/usercdn/assets/js/hover-direction-snake.min.js"></script>

    <!-- contact js -->
    <script src="~/usercdn/assets/js/contact.js"></script>
    <script src="~/usercdn/assets/js/jquery.form.js"></script>
    <script src="~/usercdn/assets/js/jquery.validate.min.js"></script>
    <script src="~/usercdn/assets/js/mail-script.js"></script>
    <script src="~/usercdn/assets/js/jquery.ajaxchimp.min.js"></script>

    <!-- Jquery Plugins, main Jquery -->
    <script src="~/usercdn/assets/js/plugins.js"></script>
    <script src="~/usercdn/assets/js/main.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/js/bootstrap.min.js" integrity="sha384-7qAoOXltbVP82dhxHAUje59V5r2YsVfBafyUDxEdApLPmcdhBPg1DKg1ERo0BZlK" crossorigin="anonymous"></script>
</body>
</html>